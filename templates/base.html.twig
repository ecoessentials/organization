<!DOCTYPE html>
<html data-theme="dark">
<head>
    <meta charset="UTF-8">
    <meta name="turbo-cache-control" content="no-cache">
    <title>{% block title %}Essentials Workflow{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body class="h-screen overflow-hidden">
<main class="w-full h-full flex flex-col overflow-hidden">
    {{ include('partials/_navigation.html.twig', {
        menu: [
            { label: 'Projets', route: 'app_project'},
            { label: 'Tiers', route: 'app_third_party'},
            { label: 'Produits', route: 'app_product'},
            { label: 'Caract√©ristiques', route: 'app_feature'}
        ]
    }) }}
    <div class="flex-grow overflow-hidden">
        {% block body %}
        {% endblock %}
    </div>
</main>
<turbo-frame id="modal-confirmation"></turbo-frame>
</body>
</html>
